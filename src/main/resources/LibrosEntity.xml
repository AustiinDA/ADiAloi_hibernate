<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Hibernate.LibrosEntity">
        <table name="libros" schema="biblioteca" catalog=""/>
        <attributes>
            <id name="idLibro">
                <column name="idLibro"/>
            </id>
            <basic name="codigoLibro">
                <column name="codigoLibro"/>
            </basic>
            <basic name="titulo">
                <column name="Titulo"/>
            </basic>
            <basic name="editorial">
                <column name="Editorial"/>
            </basic>
            <basic name="ano">
                <column name="Ano"/>
            </basic>
            <basic name="isbn">
                <column name="ISBN"/>
            </basic>
            <basic name="numEjemplares">
                <column name="NumEjemplares"/>
            </basic>
            <basic name="numPaginas">
                <column name="NumPaginas"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
